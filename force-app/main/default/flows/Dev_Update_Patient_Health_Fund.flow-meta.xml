<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <description>Record Triggered flow which sends Patient Health Fund Info to the Patient Object for Claiming purposes.</description>
    <environments>Default</environments>
    <interviewLabel>Dev Update Patient Health Fund {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Dev Update Patient Health Fund</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Update_Patient_HF_Details</name>
        <label>Update Patient HF Details</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Patient_Donor__r.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Fund_Brand_ID__c</field>
            <value>
                <elementReference>$Record.Fund_Code__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Insuring_Health_Fund_Name__c</field>
            <value>
                <elementReference>$Record.Name</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Membership_Issue_Number__c</field>
            <value>
                <elementReference>$Record.Membership_Issue_number__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Membership_Number__c</field>
            <value>
                <elementReference>$Record.Membership_Number__c</elementReference>
            </value>
        </inputAssignments>
        <object>Contact</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Update_Patient_HF_Details</targetReference>
        </connector>
        <object>Health_Fund_and_Insurance__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Draft</status>
</Flow>
