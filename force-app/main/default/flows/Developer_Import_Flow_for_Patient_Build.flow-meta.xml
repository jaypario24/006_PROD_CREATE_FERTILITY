    <formulas>
        <name>BirthDateFormula</name>
        <dataType>Date</dataType>
        <expression>IF(
  DAY({!Get_Imported_Patinet.DOB__c}) &lt;= 12 &amp;&amp; MONTH({!Get_Imported_Patinet.DOB__c}) &lt;= 12,
  DATE(
    YEAR({!Get_Imported_Patinet.DOB__c}),
    DAY({!Get_Imported_Patinet.DOB__c}),
    MONTH({!Get_Imported_Patinet.DOB__c})
  ),
  {!Get_Imported_Patinet.DOB__c}
)</expression>
    </formulas>
    <formulas>
        <name>DateConversion</name>
        <dataType>String</dataType>
        <expression>IF(
  VALUE(LEFT({!Get_Imported_Patinet.MedicareExpiry__c}, FIND(&quot;/&quot;, {!Get_Imported_Patinet.MedicareExpiry__c})-1)) &lt; 10,
  &quot;0&quot; &amp; LEFT({!Get_Imported_Patinet.MedicareExpiry__c}, FIND(&quot;/&quot;, {!Get_Imported_Patinet.MedicareExpiry__c})-1),
  LEFT({!Get_Imported_Patinet.MedicareExpiry__c}, FIND(&quot;/&quot;, {!Get_Imported_Patinet.MedicareExpiry__c})-1)
)
&amp; &quot;/&quot; &amp; RIGHT({!Get_Imported_Patinet.MedicareExpiry__c}, 2)</expression>
    </formulas>
