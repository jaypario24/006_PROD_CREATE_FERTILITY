<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Positive_Infectious_Screen__c</fullName>
    <externalId>false</externalId>
    <formula>OR(
ISPICKVAL(HIV__c, &quot;Positive&quot;),
ISPICKVAL(RPR_Syphilis__c, &quot;Positive&quot;),
ISPICKVAL(Hepatitis_B__c, &quot;Positive&quot;),
ISPICKVAL(Chlamydia_Urine__c, &quot;Positive&quot;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Positive Infectious Screen</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
