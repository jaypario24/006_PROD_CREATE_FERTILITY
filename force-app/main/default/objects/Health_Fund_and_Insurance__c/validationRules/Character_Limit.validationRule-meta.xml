<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Character_Limit</fullName>
    <active>true</active>
    <description>Limits to 2</description>
    <errorConditionFormula>LEN(Membership_Issue_number__c) &gt;2</errorConditionFormula>
    <errorDisplayField>Membership_Issue_number__c</errorDisplayField>
    <errorMessage>Too long, maximum 2 characters.</errorMessage>
</ValidationRule>
